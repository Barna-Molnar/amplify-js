{
	"compilerOptions": {
		"esModuleInterop": true,
		"noImplicitAny": false,
		"lib": [
			"dom",
			"es2017",
			"esnext.asynciterable",
			"es2018.asyncgenerator",
			"es2019"
		],
		"downlevelIteration": true,
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"declaration": true,
		"incremental": true,
		"sourceMap": true,
		"types": ["jest", "node"],
		"allowJs": true
	},
	"exclude": ["node_modules"]
}
